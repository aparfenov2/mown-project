# ------- google map node env --------

FROM kan-rt.ddns.net:8929/docker/ros-mower:mower-core-base


RUN apt-get update && apt-get install -y firefox firefox-geckodriver
RUN python3 -m pip install selenium
RUN python3 -m pip install utm
RUN apt-get update && apt-get install -y ros-noetic-foxglove-msgs
