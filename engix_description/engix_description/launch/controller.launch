<launch>

<rosparam file="$(find engix_description)/launch/controller.yaml" command="load"/>

<node args="left_wheel_joint_position_controller right_wheel_joint_position_controller right_caster_joint_position_controller left_caster_joint_position_controller joint_state_controller " name="controller_spawner" ns="engix" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
      
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/engix/joint_states"/>
</node>

</launch>
